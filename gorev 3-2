// \n i de sonraki satira gecmek icin kullandim
#include <stdio.h>

int main() {
int sayi,a=1;
printf("sayi giriniz:");
scanf("%d",&sayi);
sayi++;
for(a;a<sayi;a++){
    printf("\n%d ",a);
    printf("%d ",a*a);
    printf("%d ",a*a*a);
}

    
    
    return 0;
}
