//sayıları scanfden aldıgım sayı degiskenine kadar teker teker denemek icin sayac degiskenini kullandim
//if koşuluyla da boleni olup olmadigini anladım
#include <stdio.h>
int main() {
   int sayi,sayac=1;
    printf("sayi giriniz:");
    scanf("%d",&sayi);
    for(sayac;sayac<=sayi;sayac++){
      if(sayi%sayac==0)
      printf("\n%d",sayac);
      else
      continue;
    }
    

    return 0;
}
