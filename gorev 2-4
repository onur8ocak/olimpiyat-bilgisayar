#include <stdio.h>
int main() {
 int s1,s2,s3,s4,s5,toplam=0;
 printf("1.sayiyi giriniz");
 scanf("%d",&s1);
 printf("2.sayiyi giriniz");
 scanf("%d",&s2);
 printf("3.sayiyi giriniz");
 scanf("%d",&s3);
 printf("4.sayiyi giriniz");
 scanf("%d",&s4);
 printf("5.sayiyi giriniz");
 scanf("%d",&s5);
 if(s1%2)
 toplam+=s1;
 if(s2%2)
 toplam+=s2;
 if(s3%2)
 toplam+=s3;
 if(s4%2)
 toplam+=s4;
 if(s5%2)
 toplam+=s5;
 printf("%d",toplam);
    return 0;
}
